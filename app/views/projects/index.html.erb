<h1>All Projects</h1> <table>
  <thead> 
    <tr>
      <td>Project Name</td>
      <td>Total Project Size</td>
    </tr>
  </thead>
  <tbody>
    <% @projects.each do |project| %>
      <tr id="<%= dom_id(project) %>"j>
        <td class="name"><%= project.name %></td>
        <td class="total-size"><%= project.total_size %></td> 
      </tr>
    <% end %> 
 </tbody>
</table>

